n = int(input("How many fibonacci number do you want to list?:  "))
fibonacci = []

while len(fibonacci)<=1:
    fibonacci.append(1)
    if len(fibonacci)<=1:
        fibonacci.append(1)
    for i in range(1, n-1):
            fibonacci.append(fibonacci[i] + fibonacci[i-1])
print(f"fibonacci  ==> {fibonacci}")


